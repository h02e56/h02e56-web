    "minimist": "^1.1.0",
    "routes": "^2.0.0",
    "tcp-bind": "^1.1.1",
    "through2": "^0.6.3",
    "trumpet": "^1.7.0",
    "xtend": "^4.0.0",
    "handlebars": "1.3.0"
  },
