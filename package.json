{
  "name": "basic-env",
  "version": "0.0.1",
  "description": "basic env",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "test-js": "beefy ./public/app.js:public/js/build.js --live --index=./public/index.html --  -g hbsfy -d",
    "watch-css": "catw -w -c 'compass compile' ./public/sass/*.scss",
    "watch": "npm run watch-css & npm run test-js ",
    "build-pro": "browserify ./public/app.js -g [envify --NODE_ENV production] -g hbsfy -o > public/js/build.js"
  },
